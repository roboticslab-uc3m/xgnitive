<ECF>
	<Algorithm>


        <ParticleSwarmOptimization>
            <Entry key="weightType">0</Entry> <!-- weight type update: 0 - constant, 1 - time dependant (based on max generations) -->
            <Entry key="weight">0.8</Entry>   <!-- initial inertia weight (either constant or time dependant) -->
            <Entry key="maxVelocity">100</Entry>  <!-- max particle velocity -->
        </ParticleSwarmOptimization> 
	</Algorithm>


	<Genotype>
		<FloatingPoint>
			<Entry key="lbound">-15</Entry>
			<Entry key="ubound">90</Entry>
			<Entry key="dimension">15</Entry>
		</FloatingPoint>
	</Genotype>
	<Registry>
		<Entry key="population.size">50</Entry>
		<Entry key="mutation.indprob1">0.6</Entry> <!-- individual mutation probability (default: 0.3) -->
		<Entry key="log.level">1</Entry>
		<Entry key="log.filename">HallOfFame.xml</Entry>
		<Entry key="term.fitnessval">20</Entry> <!-- value for termination -->
	</Registry>
</ECF>


