<Robot name="right_arm_robot">

<KinBody name="right_arm" makejoinedlinksadjacent="false">
<!--This link added for static joint-->
    <body name="right_arm/clavicle">
	<Translation>0 0 0</Translation>
      <geom type="box">
        <extents>0.001 0.001 0.001</extents>
      </geom>
    </body>


    <Body name="right_arm/shoulder_roller" type="dynamic">
	<Translation>0 0 0</Translation>
	<RotationAxis>0 0 1 0</RotationAxis>
      <Geom type="trimesh">
        <Render>models/right_arm/shoulder_roller.wrl</Render>
	<collision>models/right_arm/shoulder_roller.wrl</collision>
      </Geom>
    </Body>

    <Joint circular="true" name="right_arm/shoulder_roll" type="hinge">
      <Body>right_arm/clavicle</Body>
      <Body>right_arm/shoulder_roller</Body>
      <offsetfrom>right_arm/clavicle</offsetfrom>
      <weight>4</weight>
      <limitsdeg>-180 180</limitsdeg>
      <axis>0 1 0</axis>
      <maxvel>3</maxvel>
      <resolution>1</resolution>
    </Joint>

    <Body name="right_arm/shoulder_pitcher" type="dynamic">
	<Translation>0 0 0</Translation>
	<RotationAxis>0 0 1 0</RotationAxis>
      <Geom type="trimesh">
        <Render>models/right_arm/shoulder_pitcher.wrl</Render>
	<collision>models/right_arm/shoulder_pitcher.wrl</collision>
      </Geom>
    </Body>

    <Joint circular="true" name="right_arm/shoulder_pitch" type="hinge">
      <Body>right_arm/shoulder_roller</Body>
      <Body>right_arm/shoulder_pitcher</Body>
      <offsetfrom>right_arm/shoulder_roller</offsetfrom>
      <weight>4</weight>
      <limitsdeg>-10 100</limitsdeg>
      <axis>1 0 0</axis>
      <maxvel>3</maxvel>
      <resolution>1</resolution>
    </Joint>

    <Body name="right_arm/arm" type="dynamic">
	<Translation>0 0 -0.337</Translation>
	<RotationAxis>0 0 1 0</RotationAxis>
      <Geom type="trimesh">
        <Render>models/right_arm/arm.wrl</Render>
	<collision>models/right_arm/arm.wrl</collision>
      </Geom>
    </Body>

    <Joint circular="true" name="right_arm/shoulder_yaw" type="hinge">
      <Body>right_arm/shoulder_pitcher</Body>
      <Body>right_arm/arm</Body>
      <offsetfrom>right_arm/shoulder_pitcher</offsetfrom>
      <weight>4</weight>
      <limitsdeg>-30 30</limitsdeg>
      <axis>0 0 1</axis>
      <maxvel>3</maxvel>
      <resolution>1</resolution>
    </Joint>

    <Body name="right_arm/forearm" type="dynamic">
	<Translation>0 0 -0.337</Translation>
	<RotationAxis>0 1 0 0</RotationAxis>
      <Geom type="trimesh">
        <Render>models/right_arm/forearm.wrl</Render>
	<collision>models/right_arm/forearm.wrl</collision>
      </Geom>
    </Body>

    <Joint circular="true" name="right_arm/elbow" type="hinge">
      <Body>right_arm/arm</Body>
      <Body>right_arm/forearm</Body>
      <offsetfrom>right_arm/arm</offsetfrom>
      <weight>4</weight>
      <limitsdeg>0 135</limitsdeg>
      <axis>0 1 0</axis>
      <maxvel>3</maxvel>
      <resolution>1</resolution>
    </Joint>

    <Body name="right_arm/prewrist" type="dynamic">
	<Translation>0 0 -0.547</Translation>
	<RotationAxis>0 0 1 0</RotationAxis>
      <Geom type="trimesh">
        <Render>models/right_arm/prewrist.wrl</Render>
	<collision>models/right_arm/prewrist.wrl</collision>
      </Geom>
    </Body>

    <Joint circular="true" name="right_arm/prewrist" type="hinge">
      <Body>right_arm/forearm</Body>
      <Body>right_arm/prewrist</Body>
      <offsetfrom>right_arm/forearm</offsetfrom>
      <weight>4</weight>
      <limitsdeg>-90 90</limitsdeg>
      <axis>0 0 1</axis>
      <maxvel>3</maxvel>
      <resolution>1</resolution>
    </Joint>

    <Body name="right_arm/wrist" type="dynamic">
	<Translation>0 0 -0.547</Translation>
	<RotationAxis>0 0 1 0</RotationAxis>
      <Geom type="trimesh">
        <Render>models/right_arm/wrist.wrl</Render>
	<collision>models/right_arm/wrist.wrl</collision>
      </Geom>
    </Body>

    <Joint circular="true" name="right_arm/wrist" type="hinge">
      <Body>right_arm/prewrist</Body>
      <Body>right_arm/wrist</Body>
      <offsetfrom>right_arm/prewrist</offsetfrom>
      <weight>4</weight>
      <limitsdeg>-90 90</limitsdeg>
      <axis>0 1 0</axis>
      <maxvel>3</maxvel>
      <resolution>1</resolution>
    </Joint>

  </KinBody>
</Robot>
